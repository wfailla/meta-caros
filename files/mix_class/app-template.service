[Unit]
Description=@@DESCRIPTION@@

[Service]
Environment="RELEASE_CONFIG_FILE=@@CONFFILE@@"
SyslogIdentifier=@@APPNAME@@
ExecStart=@@APP_CONTROL@@ --from-systemd=%p@%i start @@APP_PREFIX@@/@@APPNAME@@
ExecStop=@@APP_CONTROL@@ --from-systemd=%p@%i stop
Restart=always

[Install]
WantedBy=multi-user.target
